    <label asp-for="Url"></label>
    <input asp-for="Url" class="form-control" />
    <span asp-validation-for="Url"></span>
</div>
<div class="form-group">
    <label asp-for="Caption"></label>
    <input asp-for="Caption" class="form-control" />
    <span asp-validation-for="Caption"></span>
</div>
<div class="form-group">
    <label asp-for="Image" class="w-100"></label>
    <input type="text" asp-for="Image" data-type="media-selector" />
</div> 
<div class="form-group">
    <label asp-for="MobileImage" class="w-100"></label>
    <input type="text" asp-for="MobileImage" data-type="media-selector" />
</div> 